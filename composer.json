{
    "name": "kraenkvisuell/nova-cms",
    "description": "CMS for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "layout"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.3.0|^8.0",
        "coderello/laravel-nova-lang": "^1.6",
        "eminiarts/nova-tabs": "^1.3.1",
        "kraenkvisuell/nova-cms-blocks": "^1.1.1",
        "kraenkvisuell/nova-cms-media": "^2.0.1",
        "kraenkvisuell/nova-menu-builder": "^7.0",
        "laravel-lang/lang": "^10.0",
        "manogi/nova-tiptap": "^2.0",
        "optimistdigital/nova-settings": "^3.5",
        "optimistdigital/nova-translatable": "^1.11",
        "spatie/laravel-translatable": "^5.2",
        "timothyasp/nova-color-field": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Kraenkvisuell\\NovaCms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kraenkvisuell\\NovaCms\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/nova": "~3.0",
        "orchestra/testbench": "^6.17",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kraenkvisuell\\NovaCms\\NovaCmsServiceProvider"
            ]
        }
    }
}
