{
    "name": "kraenkvisuell/nova-cms",
    "description": "CMS for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "layout"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": ">=7.3.0"
    },
    "autoload": {
        "psr-4": {
            "Kraenkvisuell\\NovaCms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kraenkvisuell\\NovaCms\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/nova": "~3.0",
        "orchestra/testbench": "^5.3",
        "phpunit/phpunit": "^9.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kraenkvisuell\\NovaCms\\NovaCmsServiceProvider"
            ]
        }
    }
}
