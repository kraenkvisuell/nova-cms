{
    "name": "kraenkvisuell/nova-cms",
    "description": "CMS for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "layout"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "../nova-flexible-content"
        },
        {
            "type": "path",
            "url": "../nova-content-blocks"
        },
        {
            "type": "path",
            "url": "../nova-hero"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "eminiarts/nova-tabs": "^1.2",
        "optimistdigital/nova-menu-builder": "^2.3",
        "optimistdigital/nova-media-field": "^1.2",
        "optimistdigital/nova-translatable": "^1.5",
        "kraenkvisuell/nova-content-blocks": "*",
        "kraenkvisuell/nova-hero": "*",
        "manogi/nova-tiptap": "^1.0",
        "spatie/laravel-translatable": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Kraenkvisuell\\NovaCms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kraenkvisuell\\NovaCms\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/nova": "~3.0",
        "orchestra/testbench": "^5.3",
        "phpunit/phpunit": "^9.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kraenkvisuell\\NovaCms\\NovaCmsServiceProvider"
            ]
        }
    }
}
